
config sysmonitor 'sysmonitor'
	option enable '1'
	option led '1'
	option systime '10'
	option initial_dir 'music,video'
	option webdav_rw '0'
	option ftp '1'
	option ftp_rw '0'
	option samba '1'
	option samba_rw '0'
	option samba_rw_dir ''
	option nfs '1'
	option nfs_rw '0'
	option nfs_rw_dir ''
	option wan '192.168.1.188'
	option mask '255.255.0.0'
	option gateway '192.168.1.1'
	list dns '119.29.29.29'
	list dns '223.5.5.5'

